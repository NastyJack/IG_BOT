{
   "background": {
      "page": "/background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "48": "img/icon-badge-48-normal.png"
      },
      "default_popup": "/inssist.html?popup",
      "default_title": "INSSIST | Instagram Assistant"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "/js/emoji-regex.min.js", "/ig-content-script/index.js" ],
      "matches": [ "https://*.instagram.com/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "/js/emoji-regex.min.js", "/fb-content-script/index.js" ],
      "matches": [ "https://*.facebook.com/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com/ https://www.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_ext_description__",
   "icons": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "500": "img/icon-500.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_ext_name_production__",
   "optional_permissions": [ "*://*.onfastspring.com/*" ],
   "permissions": [ "alarms", "background", "cookies", "webRequest", "webRequestBlocking", "*://*.instagram.com/*", "*://*.facebook.com/*", "*://*.inssist.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "18.0.8",
   "web_accessible_resources": [ "/ig-injection/index.js", "/fb-injection/index.js", "/background/index.js", "/manifest.json", "/img/icon-128.png", "/img/new-post-extra-tag-assist.png", "/img/new-post-extra-cover-assist.png", "/img/rocket.png" ]
}
